{
	"extends": ["./base.json"],
	"plugins": ["react", "react-hooks", "jsx-a11y"],
	"settings": {
		"react": {
			"version": "detect"
		}
	},
	"rules": {
		"react-hooks/rules-of-hooks": "off",
		"react-hooks/exhaustive-deps": "off",
		"react/prop-types": "off",
		"react/react-in-jsx-scope": "off"
	}
}
