{
	"root": true,
	"env": {
		"browser": true,
		"es2022": true,
		"node": true
	},
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint", "import"],
	"settings": {
		"import/resolver": {
			"typescript": true,
			"node": true
		}
	},
	"rules": {
		"no-unused-vars": "off",
		"@typescript-eslint/no-unused-vars": "off",
		"import/no-unresolved": "off"
	}
}
